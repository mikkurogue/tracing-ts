{
  "name": "@tracing/core",
  "version": "0.1.0",
  "description": "A runtime-agnostic tracing library for TypeScript/JavaScript, inspired by Rust's tracing crate",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./subscribers": {
      "import": {
        "types": "./dist/subscribers.d.ts",
        "default": "./dist/subscribers.js"
      },
      "require": {
        "types": "./dist/subscribers.d.cts",
        "default": "./dist/subscribers.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "example": "bun run example.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "tracing",
    "logging",
    "observability",
    "spans",
    "structured-logging",
    "runtime-agnostic"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
